@model Stardust.Models.Proveedor

@{
    ViewBag.Title = "Buscar Proveedor";
}

<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>

@ViewBag.lol

<h2>Buscar Proveedor</h2>

@using (@Html.BeginForm("Index", "Proveedor", FormMethod.Post, new { @class = "form" }))
{


    <fieldset>
        <div class="widget">
            <div class="title">
                <h6>Buscar Proveedor</h6>
            </div>
			<div class="formRow"> @Html.LabelFor(model => model.Razon_Social)
                <div class="formRight"> @Html.EditorFor(model => model.Razon_Social) </div>
                <div class="clear"></div>
            </div>
            <div class="formRow"> @Html.LabelFor(model => model.Contacto)
                <div class="formRight"> @Html.EditorFor(model => model.Contacto) </div>
                <div class="clear"></div>
            </div>

        </div>
        <div class="formSubmit">
                <input class="redB" type="submit" value="Buscar"/>
        </div>
    </fieldset>
    
}  
    
