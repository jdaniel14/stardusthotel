@model Stardust.Models.ServiciosBean

@{
    ViewBag.Title = "Buscar Servicio";
}

<h2>BuscarServicio</h2>

<p>
    Llene el siguiente formulario para la creación de un nuevo servicio
</p>

<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>

@using (Html.BeginForm("MostrarServicios","Servicios"))

{

    <form action = "" class = "form" >
     <fieldset>


     <div class="editor-label">
                <label>Nombre</label>
            </div>
            <div class="editor-field">
                @Html.TextBoxFor(m => m.nombre)                              
            </div>

            <p>
                <input type="submit" value="Iniciar búsqueda" />
            </p>

     </fieldset>
    </form>


}