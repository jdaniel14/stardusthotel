@model Stardust.Models.AmbienteBean

@{
    ViewBag.Title = "Buscar Ambiente";
}

<h2>Buscar algún Ambiente</h2>

<p>
    Ingrese los datos para buscar el ambiente
</p>

<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>

@using (Html.BeginForm("MostrarAmbiente","Servicios"))

{

    <form action = "" class = "form" >
     <fieldset>


     <div class="editor-label">
                <label>Nombre</label>
            </div>
            <div class="editor-field">
                @Html.TextBoxFor(m => m.nombre)                              
            </div>

            <p>
                <input type="submit" value="Iniciar búsqueda" />
            </p>

     </fieldset>
    </form>


}